{
    "build": {
        "env": {
            "NODE_VERSION": "22.16.0"
        }
    },
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "https://lux-legderr.vercel.app/api/:path*"
        },
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ]
}